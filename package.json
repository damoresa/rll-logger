{
  "name": "rll-logger",
  "version": "0.0.4",
  "description": "Rll-Logger library test.",
  "main": "lib/bundle/rll-logger.umd.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "scripts": {
    "****** BUILD ******": "",
    "build": "npm run clean && npm run test && npm run tsc && npm run webpack",
    "****** CLEAN ******": "",
    "clean": "npm run clean:test && npm run clean:lib",
    "clean:all": "npm run clean && npm run clean:node_modules",
    "clean:lib": "rimraf lib",
    "clean:test": "rimraf coverage",
    "clean:node_modules": "rimraf node_modules",
    "****** TEST ******": "",
    "test": "npm run test:karma && npm run test:coverage",
    "test:karma": "karma start ./karma/karma.config.js",
    "test:coverage": "npm run test:remap && npm run test:report",
    "test:remap": "remap-istanbul -i ./coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,tests,karma",
    "test:report": "istanbul report",
    "****** COMPILE & PACKAGE ******": "",
    "tsc": "tsc",
    "webpack": "webpack"
  },
  "keywords": [
    "RedLinesLab",
    "Logger",
    "Angular2 Test"
  ],
  "author": "Damoresa",
